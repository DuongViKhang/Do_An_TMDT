@model IEnumerable<Do_an_TMDT.Models.ThuongHieu>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenThuongHieu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Slug)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenThuongHieu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Slug)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaThuongHieu">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaThuongHieu">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaThuongHieu">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
